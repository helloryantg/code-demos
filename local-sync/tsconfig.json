{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2020",
    "module": "NodeNext",
    "rootDir": "./",
    "outDir": "dist",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
}
